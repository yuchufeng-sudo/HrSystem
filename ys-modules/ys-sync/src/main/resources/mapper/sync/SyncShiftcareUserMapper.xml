<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ys.sync.mapper.SyncShiftcareUserMapper">

    <resultMap type="com.ys.sync.domain.SyncShiftcareUser" id="SyncShiftcareUserResult">
        <result property="id"    column="id"    />
        <result property="username"    column="username"    />
        <result property="password"    column="password"    />
        <result property="userId"    column="user_id"    />
        <result property="shiftcareUserId"    column="shiftcare_user_id"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectSyncShiftcareUserVo">
        select id, username, password, user_id, shiftcare_user_id, create_by, create_time, update_by, update_time, remark from sync_shiftcare_user
    </sql>

    <select id="selectSyncShiftcareUserByUserId" resultMap="SyncShiftcareUserResult">
        <include refid="selectSyncShiftcareUserVo"/>
        where user_id = #{userId}
    </select>

    <delete id="deleteSyncShiftcareUserByUserId">
        delete from sync_shiftcare_user where user_id = #{userId}
    </delete>

</mapper>
